{
  "name": "otto-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "yarn check-trans && yarn codegen:cn && next build",
    "check-types": "tsc --noemit",
    "download-schema:otto": "apollo service:download --endpoint=https://api.thegraph.com/subgraphs/name/otterclam/otto otto-graphql-schema.json",
    "download-schema:otter": "apollo service:download --endpoint=https://api.thegraph.com/subgraphs/name/otterclam/otterclam otter-graphql-schema.json",
    "codegen:contract": "typechain --target ethers-v5 --out-dir=src/contracts/__generated__ 'src/contracts/abis/*.json'",
    "codegen:otto-graph": "yarn download-schema:otto && apollo codegen:generate --includes=src/graphs/otto.ts --localSchemaFile=otto-graphql-schema.json --target=typescript --tagName=gql --globalTypesFile=./src/__generated__/otto/global-types.ts",
    "codegen:otter-graph": "yarn download-schema:otter && apollo codegen:generate --includes=src/graphs/otter.ts --localSchemaFile=otter-graphql-schema.json --target=typescript --tagName=gql --globalTypesFile=./src/__generated__/otter/global-types.ts && rm ./src/__generated__/otter/global-types.ts",
    "codegen": "yarn codegen:contract && yarn codegen:otto-graph && yarn codegen:otter-graph",
    "prepare": "husky install",
    "codegen:cn": "node ./scripts/gen-cn.mjs ./public/locales/zh-tw/common.json",
    "check-trans": "node ./scripts/validate-translation-files.mjs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{png,jpeg,jpg,gif,svg}": [
      "node ./scripts/imagemin.mjs"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@netlify/functions": "^1.0.0",
    "@react-hook/resize-observer": "^1.2.5",
    "@reduxjs/toolkit": "^1.8.1",
    "@typechain/ethers-v5": "^10.0.0",
    "@usedapp/core": "^1.1.5",
    "@web3-react/walletconnect-connector": "^6.2.10",
    "@web3-react/walletlink-connector": "^6.2.13",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "ethers": "^5.6.8",
    "graphql": "^15.0.0",
    "ioredis": "^5.0.6",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "next": "^12.2.5",
    "next-i18next": "^11.0.0",
    "next-pwa": "^5.5.4",
    "next-transpile-modules": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.1.0",
    "react-markdown": "^8.0.2",
    "react-redux": "^7.2.0",
    "react-responsive": "^9.0.0-beta.10",
    "react-responsive-carousel": "^3.2.23",
    "react-swipeable": "^7.0.0",
    "react-tooltip": "^4.2.21",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.1.12",
    "recharts-scale": "^0.4.5",
    "rehype-raw": "^6.1.1",
    "styled-components": "^5.3.3",
    "twitter-api-sdk": "^1.0.7",
    "twitter-lite": "^1.1.0",
    "typechain": "^8.0.0",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-sources": "^3.2.3",
    "workbox-core": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3",
    "workbox-window": "^6.5.3"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.9.1",
    "@types/graphql": "14.5.0",
    "@types/lodash": "^4.14.180",
    "@types/md5": "^2.3.2",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.7",
    "@types/react-transition-group": "^4.4.5",
    "@types/serviceworker": "^0.0.48",
    "@types/styled-components": "^5.1.15",
    "@types/workbox-window": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "apollo": "2.34.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "lint-staged": "^13.0.3",
    "opencc-js": "^1.0.4",
    "prettier": "^2.5.1",
    "react-error-overlay": "6.0.9",
    "sass": "^1.54.1",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9",
    "graphql": "^15.0.0"
  }
}
